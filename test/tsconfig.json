{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"noImplicitAny": false,
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"module": "CommonJS",
		"target": "ES2022",
		"rootDir": ".."
	},
	"include": [
		"./**/*.ts",
		"./**/*.js",
		"../src/**/*.ts"
	],
	"exclude": [
		"../node_modules",
		"../src/www"
	]
}
