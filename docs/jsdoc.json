{
  "source": {
      "include": ["lib/genSpecs/gen1/"], 
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
      "recurse": true,
      "destination": "docs/",
      "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}
