{
	"i18n": true,
	"type": "panel",
	"items": {
		"username": {
			"type": "text",
			"label": "Login",
			"newLine": true
		},
		"password": {
			"type": "password",
			"label": "Password"
		},
		"enable_map_creation": {
			"type": "checkbox",
			"label": "Enable Map Creation",
			"newLine": true
		},
		"map_creation_interval": {
			"type": "number",
			"label": "Map Creation Interval",
			"newLine": true,
			"min": 1,
			"max": 240,
			"style": {
			  "width": "calc(100% - 198px)"
			}
		},
		"updateInterval": {
			"type": "number",
			"label": "Update interval",
			"newLine": true,
			"min": 60,
			"max": 240,
			"style": {
			  "width": "calc(100% - 198px)"
			}
		},
		"map_scale": {
			"type": "number",
			"label": "Quality of map (higher = better)",
			"newLine": true,
			"min": 1,
			"max": 32,
			"style": {
			  "width": "calc(100% - 198px)"
			}
		},
		"webserverPort": {
			"type": "port",
			"label": "Webserver Port",
			"newLine": true,
			"style": {
			  "width": "calc(100% - 198px)"
			}
		},
		"hostname_ip": {
			"type": "text",
			"label": "Hostname/IP for camera stream",
			"newLine": true
		},
		"cameraPin": {
			"type": "number",
			"label": "PIN for streaming the camera",
			"newLine": true
		},
		"downloadRoborockImages": {
			"type": "checkbox",
			"label": "Download Roborock Images",
			"newLine": true
		}
	}
}
